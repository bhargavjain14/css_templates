{
  field: 'numericField', // Replace with your actual field name
  filter: 'agNumberColumnFilter',
  filterParams: {
    comparator: function(filterLocalNumber, cellValue) {
      // Check if cellValue is null or undefined
      if (cellValue == null) {
        return -1; // or any value that suits your needs
      }

      // Convert cellValue to a number if it's a string
      if (typeof cellValue === 'string') {
        cellValue = Number(cellValue);
      }

      if (cellValue < filterLocalNumber) {
        return -1;
      } else if (cellValue > filterLocalNumber) {
        return 1;
      } else {
        return 0;
      }
    }
  },
  comparator: function(valueA, valueB, nodeA, nodeB, isInverted) {
    // Check if valueA or valueB is null or undefined
    if (valueA == null) return -1;
    if (valueB == null) return 1;

    // Convert valueA and valueB to numbers if they're strings
    if (typeof valueA === 'string') {
      valueA = Number(valueA);
    }
    if (typeof valueB === 'string') {
      valueB = Number(valueB);
    }

    return valueA - valueB;
  }
}
