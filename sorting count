const subjectDetails = [
  { subject: "science", name: "aaa" },
  { subject: "math", name: "aaa" },
  { subject: "bio", name: "aaa" },
  { subject: "science", name: "aaa" }
];

const countMap = new Map<string, number>();

subjectDetails.forEach(item => {
  const subject = item.subject.toLowerCase();
  countMap.set(subject, (countMap.get(subject) || 0) + 1);
});

const result = Array.from(countMap.entries()).map(([subject, count]) => ({
  id: subject,
  value: count
}));

console.log(result);
